{
  "name": "rustshark",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "cd frontend && npm run dev:integrated",
    "frontend": "cd frontend && npm run dev",
    "backend": "cd backend && cargo run --bin rustshark",
    "build": "cd frontend && npm run build",
    "install:all": "npm install && cd frontend && npm install"
  },
  "workspaces": [
    "frontend"
  ]
}
